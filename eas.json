{
  "cli": {
    "version": ">= 16.2.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": false,
      "distribution": "internal",
      "android": { "buildType": "apk" }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
      }
    },
    "production": {
      "autoIncrement": true,
      "android":{
        "buildType": "aab",
        "gradleCommand": ":app:assembleRelease"
      }
    }
    
  },
  "submit": {
    "production": {}
  }
  
}
